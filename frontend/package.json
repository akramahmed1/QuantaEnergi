{
  "name": "quantaenergi-frontend",
  "version": "4.0.0",
  "description": "QuantaEnergi Frontend - Next-Gen ETRM/CTRM Trading Dashboard",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next out dist",
    "postinstall": "husky install"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    
    "lucide-react": "^0.292.0",
    "clsx": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "tailwind-merge": "^2.0.0",
    
    "jwt-decode": "^3.1.2",
    "axios": "^1.6.0",
    "react-query": "^3.39.0",
    "react-hook-form": "^7.47.0",
    "@hookform/resolvers": "^3.3.0",
    "zod": "^3.22.0",
    
    "react-hot-toast": "^2.4.0",
    "react-intersection-observer": "^9.5.0",
    "framer-motion": "^10.16.0",
    "react-spring": "^9.7.0",
    
    "date-fns": "^2.30.0",
    "react-datepicker": "^4.21.0",
    "@types/react-datepicker": "^4.19.0",
    
    "recharts": "^2.8.0",
    "react-table": "^7.8.0",
    "@types/react-table": "^7.7.0",
    
    "react-dropzone": "^14.2.0",
    "react-csv": "^2.2.0",
    "xlsx": "^0.18.0",
    
    "socket.io-client": "^4.7.0",
    "react-use-websocket": "^4.5.0",
    
    "react-i18next": "^13.5.0",
    "i18next": "^23.7.0",
    "i18next-browser-languagedetector": "^7.2.0",
    
    "react-helmet-async": "^1.3.0",
    "next-seo": "^6.4.0",
    
    "react-error-boundary": "^4.0.0",
    "react-error-overlay": "^7.0.0",
    
    "workbox-webpack-plugin": "^7.0.0",
    "next-pwa": "^5.6.0"
  },
  "devDependencies": {
    "@next/eslint-config-next": "^14.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-import": "^2.29.0",
    
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    
    "cypress": "^13.4.0",
    "@cypress/react": "^6.0.0",
    "@cypress/webpack-dev-server": "^3.0.0",
    
    "@storybook/react": "^7.5.0",
    "@storybook/react-vite": "^7.5.0",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.0",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/blocks": "^7.5.0",
    "@storybook/testing-library": "^0.2.0",
    
    "cross-env": "^7.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0",
    
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    
    "webpack-bundle-analyzer": "^4.9.0",
    "source-map-explorer": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{js,jsx,ts,tsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akramahmed1/EnergyOpti-Pro.git"
  },
  "keywords": [
    "etrm",
    "ctrm",
    "energy-trading",
    "commodity-trading",
    "risk-management",
    "quantum-optimization",
    "ai-trading",
    "blockchain",
    "islamic-finance",
    "sharia-compliant",
    "react",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "author": "QuantaEnergi Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akramahmed1/EnergyOpti-Pro/issues"
  },
  "homepage": "https://quantaenergi.com"
}